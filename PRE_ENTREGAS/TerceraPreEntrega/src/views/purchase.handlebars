<a class='cartId' id='{{idCart}}' href='http://localhost:8080/auth/perfil'><Button>Ver Perfil</Button></a>

<h1>User: {{infoUser.email}}</h1>
<h1>Carrito</h1>
<h2>Cart ID: {{idCart}}</h2>
<h3>Products:</h3>
<ul>
  {{#each cart}}
    <li>
      <h4>title: {{title}}</h4>
      <h4>quantity: {{quantity}}</h4>
    </li>
  {{/each}}

  <h4>Total: ${{precioTotal}}</h4>
</ul>
<a href='/products'><button>ir a Productos</button></a>
<a href='http://localhost:8080/carts/{{idCart}}/purchase/ticket'><button>Finalizar Compra</button></a>